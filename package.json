  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "test": "vitest",
    "type-check": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook --url http://127.0.0.1:6006",
    "vrt": "reg-suit run",
    "prisma": "prisma",
    "prisma generate": "pnpm prisma generate",
    "prisma:reset": "pnpm prisma db push --force-reset && pnpm prisma db seed",
    "prisma:studio": "pnpm prisma studio",
    "chromatic": "chromatic --exit-once-uploaded",
    "e2e": "pnpm prisma:reset && playwright test",
    "e2e:ui": "pnpm prisma:reset && playwright test --ui",
    "e2e:debug": "pnpm prisma:reset && playwright test --debug",
    "e2e:report": "pnpm prisma:reset && playwright show-report",
    "e2e:codegen": "playwright codegen",
    "scaffdog": "scaffdog generate"
  },
