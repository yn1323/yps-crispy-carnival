{
  "mcpServers": {
    "context7": {
      "command": "cmd",
      "args": ["/c", "npx", "@upstash/context7-mcp@latest"]
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@executeautomation/playwright-mcp-server"]
    },
    "serena": {
      "command": "cmd",
      "args": [
        "/c",
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--project",
        "$(pwd)"
      ]
    }
  }
}
